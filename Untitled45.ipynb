    "from scipy import stats as st\n",
    "import math as mt\n",
    "\n",
    "binom_n = 23000\n",
    "binom_p = 0.4\n",
    "threshold = 9000\n",
    "mu = binom_n * binom_p\n",
    "sigma = (binom_n * binom_p * (1 - binom_p)) ** 0.5\n",
    "p_threshold = st.norm(mu, sigma).cdf(1 - threshold)\n",
    "\n",
    "print(p_threshold)"
   ]
